      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8F7CA0D7-1329-407E-A5D4-D9FC09F200EF}"
          SQLTask:SqlStatementSource="IF OBJECT_ID('dbo.VW_DISB_PRJCTN_ELIGIBILITY') IS NOT NULL&#xA;&#x9;DROP VIEW VW_DISB_PRJCTN_ELIGIBILITY&#xA;GO&#xA;&#xA;CREATE VIEW VW_DISB_PRJCTN_ELIGIBILITY&#xA;AS&#xA;WITH CURRENT_DISBURSEMENT_EXPIRATION AS&#xA;(SELECT FK_CONTRACT_ID&#xA;&#x9;,CURRENT_EXPIRATION_DATE&#xA;&#x9;,ROW_NUMBER() OVER (PARTITION BY FK_CONTRACT_ID ORDER BY CLAUSE_INDIVIDUAL_ID DESC) AS RN&#xA;FROM CLAUSE_CONTRACT&#xA;JOIN CLAUSE ON CLAUSE_ID = CLAUSE_CONTRACT.FK_CLAUSE_ID&#xA;JOIN CONVERGENCE_MASTER_DATA AS CLAUSE_TYPE ON CLAUSE_TYPE.CONVERGENCE_MASTER_DATA_ID = CLAUSE.FK_CATEGORY_ID&#xA;JOIN CLAUSE_INDIVIDUAL ON CLAUSE_INDIVIDUAL.FK_CLAUSE_ID = CLAUSE_ID&#xA;WHERE CLAUSE_TYPE.CODE = 'LASTD'),&#xA;&#xA;TOTAL_PROJECTED AS&#xA;(SELECT FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;,SUM(AMOUNT) AS AMOUNT&#xA;FROM DISBURSEMENT_PROJECTION&#xA;GROUP BY FK_DISBURSEMENT_PROJECTION_BASE_ID),&#xA;&#xA;DISBURSEMENT_PROJECTIONS AS&#xA;(SELECT FK_OPERATION_ID&#xA;&#x9;,FK_CONTRACT_ID&#xA;&#x9;,AMOUNT + PLANNED_CANCELED_AMOUNT AS PROJECTIONS&#xA;FROM DISBURSEMENT_PROJECTION_BASE&#xA;JOIN TOTAL_PROJECTED ON FK_DISBURSEMENT_PROJECTION_BASE_ID = DISBURSEMENT_PROJECTION_BASE_ID)&#xA;&#xA;SELECT OPERATION_ID&#xA;&#x9;,OPERATION_NUMBER&#xA;&#x9;,CONTRACT_ID&#xA;&#x9;,CONTRACT_DNUM&#xA;&#x9;,CONTRACT_NUMBER&#xA;&#x9;,IIF((OPERATION_TYPE.CODE = 'LON' AND FUND_CODE IN ('ORC','BLD','GRF','VTF','MSF','SQP'))&#xA;&#x9;&#x9;&#x9;OR (OPERATION_TYPE.CODE = 'TCP' AND FUND.IDB_CONTROLS_FUND_LIQUIDITY = 1)&#xA;&#x9;&#x9;&#x9;OR (OPERATION_TYPE.CODE = 'IGR' AND (FUND_CODE IN ('ORC','BLD','GRF','VTF','MSF','SQP') AND TOTAL_COST_CURRENT &gt;= 3000000) OR TOTAL_COST_CURRENT &lt; 3000000 AND FUND.IDB_CONTROLS_FUND_LIQUIDITY = 1) &#xA;&#x9;&#x9;,IIF(CONTRACT_STATUS.CODE NOT IN ('CO','CA','FD','PC','DE','CF') &#xA;&#x9;&#x9;&#x9;&#x9;AND COALESCE(CURRENT_EXPIRATION_DATE, ORIGINAL_EXPIRATION_DATE) &gt;= GETDATE()&#xA;&#x9;&#x9;&#x9;&#x9;AND COALESCE(CONTRACT_PROJECTIONS.PROJECTIONS, OPERATION_PROJECTIONS.PROJECTIONS, 0) &gt; 0&#xA;&#x9;&#x9;&#x9;,1&#xA;&#x9;&#x9;&#x9;,0)&#xA;&#x9;&#x9;,1) IS_DIS_PRJCNT_ELIGIBLE&#xA;FROM CONTRACT&#xA;JOIN OPERATION ON OPERATION_ID = CONTRACT.FK_OPERATION_ID&#xA;JOIN OPERATION_DATA ON OPERATION_DATA_ID = FK_OPERATION_DATA_ID&#xA;JOIN CONVERGENCE_MASTER_DATA AS OPERATION_TYPE ON OPERATION_TYPE.CONVERGENCE_MASTER_DATA_ID = OPERATION_DATA.FK_OPERATION_TYPE_ID&#xA;JOIN CONVERGENCE_MASTER_DATA AS CONTRACT_STATUS ON CONTRACT_STATUS.CONVERGENCE_MASTER_DATA_ID = CONTRACT.FK_LOAN_STATUS&#xA;LEFT JOIN CURRENT_DISBURSEMENT_EXPIRATION ON CURRENT_DISBURSEMENT_EXPIRATION.FK_CONTRACT_ID = CONTRACT_ID&#xA;&#x9;AND CURRENT_DISBURSEMENT_EXPIRATION.RN = 1&#xA;LEFT JOIN DISBURSEMENT_PROJECTIONS AS CONTRACT_PROJECTIONS ON CONTRACT_PROJECTIONS.FK_CONTRACT_ID = CONTRACT_ID&#xA;LEFT JOIN DISBURSEMENT_PROJECTIONS AS OPERATION_PROJECTIONS ON OPERATION_PROJECTIONS.FK_OPERATION_ID = OPERATION_ID &#xA;&#x9;AND CONTRACT_PROJECTIONS.FK_CONTRACT_ID IS NULL&#xA;&#x9;AND OPERATION_PROJECTIONS.FK_CONTRACT_ID IS NULL&#xA;JOIN FUND_OPERATION ON FUND_OPERATION.FK_OPERATION_ID = OPERATION_ID&#xA;&#x9;AND FUND_OPERATION.APPROVAL_NUMBER = CONTRACT_NUMBER&#xA;JOIN FUND ON FUND_ID = FUND_OPERATION.FK_FUND_ID&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
