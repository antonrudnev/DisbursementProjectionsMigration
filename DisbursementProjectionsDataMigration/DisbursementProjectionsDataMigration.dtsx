      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8F7CA0D7-1329-407E-A5D4-D9FC09F200EF}"
          SQLTask:SqlStatementSource="IF (SELECT COUNT(*) FROM TMP_MIG_DSBPRJ_ISSUES) = 0 DROP TABLE TMP_MIG_DSBPRJ_ISSUES;&#xA;&#xA;GO&#xA;&#xA;DECLARE @MINY INT, @MAXY INT;&#xA;&#xA;SELECT @MINY=MIN(YEAR), @MAXY=MAX(YEAR)&#xA;FROM DISBURSEMENT_PROJECTION;&#xA;&#xA;WITH M AS &#xA;(SELECT 1 AS MONTH&#xA;UNION ALL&#xA;SELECT MONTH+1&#xA;FROM M&#xA;WHERE MONTH&lt;12),&#xA;&#xA;Y AS&#xA;(SELECT @MINY AS YEAR&#xA;UNION ALL&#xA;SELECT YEAR+1&#xA;FROM Y&#xA;WHERE YEAR&lt;@MAXY),&#xA;&#xA;YM AS&#xA;(SELECT YEAR&#xA;&#x9;,MONTH&#xA;&#x9;,CAST(CONCAT(YEAR,FORMAT(MONTH, '0#')) AS INT) AS YM&#xA;FROM Y CROSS JOIN M),&#xA;&#xA;DSBPRJ AS&#xA;(SELECT DISBURSEMENT_PROJECTION_ID&#xA;&#x9;,FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;,CAST(CONCAT(YEAR,FORMAT(MONTH, '0#')) AS INT) AS YM&#xA;FROM DISBURSEMENT_PROJECTION),&#xA;&#xA;DSBPRJ_MIN_MAX AS&#xA;(SELECT DISTINCT FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;,MIN(YM) OVER (PARTITION BY FK_DISBURSEMENT_PROJECTION_BASE_ID) AS MIN_YM&#xA;&#x9;,MAX(YM) OVER (PARTITION BY FK_DISBURSEMENT_PROJECTION_BASE_ID) AS MAX_YM&#xA;FROM DSBPRJ),&#xA;&#xA;FULL_GRID AS&#xA;(SELECT FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;,YEAR&#xA;&#x9;,MONTH&#xA;FROM DSBPRJ_MIN_MAX&#xA;CROSS JOIN YM&#xA;WHERE YM&gt;=MIN_YM&#xA;&#x9;AND YM&lt;=MAX_YM)&#xA;&#xA;INSERT INTO DISBURSEMENT_PROJECTION (FK_DISBURSEMENT_PROJECTION_BASE_ID, YEAR, MONTH, AMOUNT, CREATED, CREATED_BY)&#xA;SELECT FULL_GRID.FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;,FULL_GRID.YEAR&#xA;&#x9;,FULL_GRID.MONTH&#xA;&#x9;,0&#xA;&#x9;,GETDATE()&#xA;&#x9;,'DP_MIGRATION'&#xA;FROM FULL_GRID&#xA;LEFT JOIN DISBURSEMENT_PROJECTION&#xA;&#x9;ON FULL_GRID.FK_DISBURSEMENT_PROJECTION_BASE_ID = DISBURSEMENT_PROJECTION.FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;&#x9;AND FULL_GRID.YEAR = DISBURSEMENT_PROJECTION.YEAR&#xA;&#x9;&#x9;AND FULL_GRID.MONTH = DISBURSEMENT_PROJECTION.MONTH&#xA;WHERE DISBURSEMENT_PROJECTION.FK_DISBURSEMENT_PROJECTION_BASE_ID IS NULL&#xA;ORDER BY FULL_GRID.FK_DISBURSEMENT_PROJECTION_BASE_ID&#xA;&#x9;,FULL_GRID.YEAR&#xA;&#x9;,FULL_GRID.MONTH&#xA;OPTION(MAXRECURSION 100);&#xA;&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
