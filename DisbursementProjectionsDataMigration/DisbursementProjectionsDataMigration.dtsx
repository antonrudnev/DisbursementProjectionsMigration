          SQLTask:SqlStatementSource="/******************************************/&#xA;--  Date: 02/10/2017&#xA;--  Jira: WLMSREPL-191&#xA;--  Release: 7.5&#xA;/******************************************/&#xA;&#xA;DECLARE @TMP_MENU_CONFIG_OPER_STAGE AS TABLE&#xA;    (&#xA;      [STAGE_CODE] NVARCHAR(50)&#xA;    );&#xA;&#xA;DECLARE @OPERATION_TYPE_ID INT;&#xA;&#xA;IF NOT EXISTS (SELECT 1 FROM MENU_CONFIG_OPER_TYPE WHERE FK_MENU_ITEM_ID = (SELECT TOP(1) MENU_ITEM_ID from MENU_ITEM where TITLE = 'Disbursement Projection') AND OPERATION_TYPE_CODE = 'TCP')&#xA;BEGIN&#xA;INSERT INTO MENU_CONFIG_OPER_TYPE &#xA;(&#xA;    FK_MENU_ITEM_ID, OPERATION_TYPE_CODE&#xA;) &#xA;VALUES &#xA;(&#xA;    (SELECT TOP(1) MENU_ITEM_ID from MENU_ITEM where TITLE = 'Disbursement Projection'),&#xA;    'TCP'&#xA;)&#xA;END&#xA;&#xA;INSERT  INTO @TMP_MENU_CONFIG_OPER_STAGE&#xA;        ( STAGE_CODE&#xA;        )&#xA;        SELECT  STAGE.OPERATION_STAGE_CODE&#xA;        FROM    MENU_CONFIG_OPER_STAGE STAGE&#xA;                CROSS JOIN MENU_CONFIG_OPER_TYPE OPER_TYPE&#xA;        WHERE   OPER_TYPE.FK_MENU_ITEM_ID = ( SELECT    MENU_ITEM_ID&#xA;                                      FROM      dbo.MENU_ITEM&#xA;                                      WHERE     TITLE = 'Outputs_TCM'&#xA;                                    )&#xA;                AND STAGE.FK_MENU_CONFIG_OPER_TYPE_ID = OPER_TYPE.MENU_CONFIG_OPER_TYPE_ID&#xA;                AND OPER_TYPE.OPERATION_TYPE_CODE = 'TCP'&#xA;&#xA;SET @OPERATION_TYPE_ID = (SELECT MENU_CONFIG_OPER_TYPE_ID FROM MENU_CONFIG_OPER_TYPE WHERE FK_MENU_ITEM_ID = (SELECT TOP(1) MENU_ITEM_ID from MENU_ITEM where TITLE = 'Disbursement Projection') AND OPERATION_TYPE_CODE = 'TCP')&#xA;&#xA;IF NOT EXISTS (SELECT 1 FROM MENU_CONFIG_OPER_STAGE WHERE FK_MENU_CONFIG_OPER_TYPE_ID = (SELECT MENU_CONFIG_OPER_TYPE_ID FROM MENU_CONFIG_OPER_TYPE WHERE FK_MENU_ITEM_ID = (SELECT TOP(1) MENU_ITEM_ID from MENU_ITEM where TITLE = 'Disbursement Projection') AND OPERATION_TYPE_CODE = 'TCP') AND OPERATION_STAGE_CODE = 'default')&#xA;BEGIN&#xA;INSERT INTO MENU_CONFIG_OPER_STAGE &#xA;(&#xA;    FK_MENU_CONFIG_OPER_TYPE_ID, &#xA;    OPERATION_STAGE_CODE&#xA;) &#xA;&#x9;SELECT DISTINCT&#xA;&#x9;@OPERATION_TYPE_ID,&#xA;&#x9;[STAGE_CODE]&#xA;&#x9;FROM @TMP_MENU_CONFIG_OPER_STAGE&#xA;END&#xA;&#xA;IF NOT EXISTS (SELECT 1 FROM MENU_CONFIG_ATTRIBUTE WHERE FK_MENU_CONFIG_OPER_TYPE_ID = (SELECT MENU_CONFIG_OPER_TYPE_ID FROM MENU_CONFIG_OPER_TYPE WHERE FK_MENU_ITEM_ID = (SELECT TOP(1) MENU_ITEM_ID from MENU_ITEM where TITLE = 'Disbursement Projection') AND OPERATION_TYPE_CODE = 'TCP') AND ATTRIBUTE_CODE = 'default' AND ATTRIBUTE_VALUE = 'default')&#xA;BEGIN&#xA;&#xA;INSERT INTO MENU_CONFIG_ATTRIBUTE &#xA;(&#xA;&#x9;FK_MENU_CONFIG_OPER_TYPE_ID, &#xA;&#x9;ATTRIBUTE_CODE, &#xA;&#x9;ATTRIBUTE_VALUE&#xA;) &#xA;VALUES &#xA;(&#xA;    (SELECT MENU_CONFIG_OPER_TYPE_ID FROM MENU_CONFIG_OPER_TYPE WHERE FK_MENU_ITEM_ID = (SELECT TOP(1) MENU_ITEM_ID from MENU_ITEM where TITLE = 'Disbursement Projection') AND OPERATION_TYPE_CODE = 'TCP'),&#xA;    'default', &#xA;    'default'&#xA;)&#xA;END&#xA;&#xA;--(21 row(s) affected)&#xA;&#xA;--(21 row(s) affected)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
